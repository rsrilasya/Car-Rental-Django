	<div id="column-634800114b68b" class="fw-col-sm-8 fw-col-md-9 tf-sh-2a9f74131d1a5766915303d3f793363a  fw-animated-element" data-animation-type="fadeInUp" data-animation-delay="700">
	<div class="fw-wrap-col-inner clearfix  "   >
				<div class="fw-col-inner">
			<div class="fw-raw-code ">
	<form id="bento-contact-form" onsubmit="event.preventDefault();">

<input type="hidden" id="bento-contact-form-website" value="Infinity Hospitality">
<input type="hidden" id="bento-redirect-url" value="https://infinityhospitalitygroup.com/thank-you/">

