<script>
var bentoToken = '6a3d4ddeaac585d8f879f39db5698266081c4b332cc5f08b799b5ac136dec22d18e9de643dcb87d7b768c793d2f63cd6facefd44120dd43f33a645f9d1044a32';
var appConfig = {
		files:{
				read:'',
				write:'',
				post:''
		},
		instance:'infinity',
		db:{
			post: 'https://api.voltz.software/_repos/_production/notify/pagoda/_coredev/_post.php?',
			read: 'https://api.voltz.software/_repos/_production/notify/pagoda/_coredev/_get.php?',
			write: 'https://api.voltz.software/_repos/_production/notify/pagoda/_coredev/_get.php?'
		},
		dom:'.pagoda-div',
		debug:false,
		form:'#contactForm',
		formButton:'.formAction',
		api_webform:true
	};
</script>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"   integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="   crossorigin="anonymous"></script>
<script src="https://bento.voltz.software/js/embed.pagoda.min.js"></script>

<script>

Factory.register('voltz-software-form', function(sb){
	
	var components = {},
		domObj = {},
		formContainer = appConfig.form,
		formButtonContainer = appConfig.formButton,
		userObj = {};
				
	return {
		
		init: function(){
			
			sb.data.db.setAppConfig(appConfig);
			
			sb.data.db.setToken(bentoToken);
			
			sb.listen({
				'voltz-software-form-load':this.load
			});
			
			sb.notify({
				type:'voltz-software-form-load',
				data:{
					buttonDomObj: sb.dom.make(formButtonContainer)
				}
			});
			
		},
		
		load: function(setup){
			
			function checkForContact(email, phone, callback){
				
				sb.data.db.obj.getWhere('contact_info', {info:email}, function(contactInfo){
					
					if(contactInfo[0]){
						
						sb.data.db.obj.getById('contacts', contactInfo[0].object_id, function(contact){
							callback(contact);
						});
						
					}else{
						
						sb.data.db.obj.getWhere('contact_info', {info:phone}, function(contactInfo){
							
							if(contactInfo[0]){
								
								sb.data.db.obj.getById('contacts', contactInfo[0].object_id, function(contact){
									callback(contact);
								});
								
							}else{
								callback(false);
							}
							
						});
						
					}
					
				});
				
			}
							
			sb.data.db.obj.getWhere('contact_info_options', {contact_info_type:1828172}, function(sourceOptions){
				
				var sourceSelectOptions = '<option value="null">Please Select</option>';
				_.each(sourceOptions, function(option){
					
					sourceSelectOptions += '<option value="'+ option.name +'">'+ option.name +'</option>';
					
				});
				
				$('#source').html(sourceSelectOptions);	
				
				$('.bento-submit-btn').on('click', function(e){
					
					var redirectURL = $('#bento-redirect-url').val();
					var formData = {
							fname: $('#fname').val(),
							lname: $('#lname').val(),
							email: $('#email').val(),
							phone: $('#phone').val(),
							source: $('#source').val(),
							eventType: $('#event-type').val(),
							requestedDate: $('#requested-date').val(),
							guestCount: $('#guest-count').val(),
							notes: $('#notes').val(),
							website: $('#bento-contact-form-website').val()
						};
						
					var formCompleted = true;
					_.each(formData, function(val,key){
						
						if(key !== 'notes' && key !== 'requestedDate'){
							if(val == '' || val == 'null'){
								formCompleted = false;
							}
						}
						
					});
						
					if(formCompleted === false){
						alert('Please fill out the entire form.');
						return;
					}
							
					$('.bento-submit-btn').html('Submitting...');
					
					checkForContact(formData.email, formData.phone, function(createdContact){
													
						if(createdContact){
							
							sb.data.db.obj.getWhere('contact_info', {object_id:createdContact.id}, function(contactInfo){
								
								var newContactInfo = [];
								var updateContactInfo = [];
								
								if(_.where(contactInfo, email).length > 0){
									newContactInfo.push(
										{
											info:formData.email,
											title:'Email Address',
											type:1119285,
											is_primary:'yes'
										}
									);
								}
								
								if(_.where(contactInfo, phone).length > 0){
									newContactInfo.push(
										{
											info:formData.email,
											title:'Phone Number',
											type:1119281,
											is_primary:'yes'
										}
									);
								}
								
								if(_.where(contactInfo, formData.source).length > 0){
									newContactInfo.push(
										{
											info:formData.source,
											title:'Lead Source',
											type:1828172,
											is_primary:'yes'
										}
									);
								}
								
								var newNote = {
										note:'New Website Submission<br />'+
											'<br />Name: '+ formData.fname +' '+ formData.lname +
											'<br />Email: '+ formData.email +
											'<br />Phone: '+ formData.phone +
											'<br />Event Type: '+ formData.eventType +
											'<br />Requested Date: '+ formData.requestedDate +
											'<br />Estimated Guest Count: '+ formData.guestCount +
											'<br />Website Visited: '+ formData.website +
											'<br />Lead Source: '+ formData.source +
											'<br />Additional Notes:<br />'+ formData.notes,
										notify: [],
										type_id:createdContact.id,
										type:'contacts'
									};
								
								if(newContactInfo.length > 0){
									
									sb.data.db.obj.create('contact_info', newContactInfo, function(contactInfoObjs){
										
										createdContact.contact_info = createdContact.contact_info.concat(_.pluck(contactInfoObjs, 'id'));

										sb.data.db.obj.update('contacts', {
											id:createdContact.id
											, contact_info:createdContact.contact_info
											, lead_source: formData.source
										}, function(updatedContact){

											sb.data.db.obj.create('notes', newNote, function(noteObj){
												
												var link = sb.data.url.createPageURL(
														'object',
														{
															type:'state',
															id:createdContact.id,
															name:formData.fname +' '+ formData.lname
														}
													);
												
												sb.data.db.obj.updateState({
													objectId: 		createdContact.id,
													newState: 		1,
													link:			link,
													stateProperty:	'state'
												}, function(updated){
													
													window.location.href = redirectURL;
													
												});
												
											});
											
										});
										
									});
									
								}
								
							});
							
						}else{
							
							var newCompany = {
									name:formData.fname +' '+ formData.lname,
									type:1897807
								};
							var newContact = {
									fname:formData.fname,
									lname:formData.lname,
									type:1929600
								};
							var newContactInfo = [
								{
									info:formData.email,
									title:'Email Address',
									type:1119285,
									is_primary:'yes'
								},
								{
									info:formData.phone,
									title:'Phone Number',
									type:1119281,
									is_primary:'yes'
								},
								{
									info:formData.source,
									title:'Lead Source',
									type:1828172,
									is_primary:'yes'
								}
							];
							var newNote = {
									note:'New Website Submission<br />'+
										'<br />Name: '+ formData.fname +' '+ formData.lname +
										'<br />Email: '+ formData.email +
										'<br />Phone: '+ formData.phone +
										'<br />Event Type: '+ formData.eventType +
										'<br />Requested Date: '+ formData.requestedDate +
										'<br />Estimated Guest Count: '+ formData.guestCount +
										'<br />Website Visited: '+ formData.website +
										'<br />Lead Source: '+ formData.source +
										'<br />Additional Notes:<br />'+ formData.notes,
									notify: [2680879],
									object_uid:0
								};
								
							sb.data.db.obj.create('companies', newCompany, function(companyObj){
								
								newContact.company = companyObj.id;
								
								sb.data.db.obj.create('contacts', newContact, function(contactObj){
									
									_.each(newContactInfo, function(info){
										info.object_id = contactObj.id;
									});
									
									sb.data.db.obj.create('contact_info', newContactInfo, function(contactInfoObjs){

										contactObj.contact_info = _.pluck(contactInfoObjs, 'id');

										sb.data.db.obj.update('contacts', {
											id:contactObj.id
											, contact_info:contactObj.contact_info
											, lead_source: formData.source
										}, function(updatedContactObj){

											newNote.type_id = contactObj.id;
											newNote.type = 'contacts';
											
											sb.data.db.obj.create('notes', newNote, function(noteObj){
												
												var link = sb.data.url.createPageURL(
														'object',
														{
															type:'state',
															id:contactObj.id,
															name:contactObj.fname +' '+ contactObj.lname
														}
													);
												
												sb.data.db.obj.updateState({
													objectId: 		contactObj.id,
													newState: 		1,
													link:			link,
													stateProperty:	'state'
												}, function(updated){
													
													window.location.href = redirectURL;
													
												});
												
											});
											
										});
										
									});
									
								});
								
							});
							
						}
						
					});	
						
				});
				
			});
							
		}
		
	}
	
});

Factory.run();

</script></div>		</div>
	</div>
